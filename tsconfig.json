{
    "compilerOptions": {
        "outDir": "./dist",
        "rootDir": "./lib",
        "target": "ES2021",
        "module": "ES2022",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "strict": true,
        "skipLibCheck": true,
    }
}
